{
  "version": "2.0",
  "functionTimeout": "00:10:00",
  "cors": {
    "allowedOrigins": ["https://localhost:3000", "https://localhost:8080", "https://*.netlify.app", "https://*.vercel.app", "https://*.azurestaticapps.net"],
    "allowedMethods": ["GET", "POST", "OPTIONS"],
    "allowedHeaders": ["Content-Type", "Authorization", "X-Requested-With"],
    "maxAge": 86400
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "default": "Information",
      "Function": "Information",
      "Host.Results": "Warning"
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.*, 4.0.0)"
  }
}